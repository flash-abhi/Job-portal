 
      <div class="row">
        <div class="col-md-12">
          <h1 class="mt-5">User Data</h1>
          <table class="table table-bordered">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Email</th>
                <th scope="col">Contact</th>
                <th scope="col">Resume</th>
              </tr>
            </thead>
            <tbody>
              <% users.forEach(user =>{ %>
              <tr>
                <th scope="row"><%= user.userId %></th>
                <td><%= user.name %></td>
                <td><%= user.email %></td>
                <td><%= user.phone %></td>
                <td><a href="<%= user.resume %>" target="_blank" class="btn btn-primary">View</a></td>
              </tr>
              <% }) %>
            </tbody>
          </table>
        </div>
      </div>